[
  {
    "name": "DependencyCheck",
    "type": "SimpleExecutable",
    "stage": "execution",
    "config": {
      "conditions": [
        "Prescription.Security.Activities.Sast.Enabled"
      ],
      "command": [
        "sh",
        "dependency-check.sh"
      ],
      "optional": [
        "Project.Branch.Name"
      ],
      "require": [
        "Project.Name"
      ],
      "success": {
        "Resources.DependencyCheck.Success": true,
        "Resources.DependencyCheck.Enabled": true
      },
      "failure": {
        "Resources.DependencyCheck.Success": false,
        "Resources.DependencyCheck.Enabled": true
      }
    }
  }
]
